<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="revisar_procesos_produccion.proceso_window_tree">
      <field name="name">Procesos</field>
      <field name="model">revisar_procesos_produccion.proceso</field>
      <field name="arch" type="xml">
        <tree>
          <field name="id_process"/>
          <field name="name"/>
          <field name="process_description"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="revisar_procesos_produccion.action_window_proceso">
      <field name="name">Procesos</field>
      <field name="res_model">revisar_procesos_produccion.proceso</field>
      <field name="view_mode">tree,form</field>
    </record>


